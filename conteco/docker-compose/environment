#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export CONTECO_DC_GLOBAL_STACKS="dotnetcore_runtime22.base"
export CONTECO_DC_GLOBAL_STACKNAMES="conteco_dc_dotnetcore_runtime22"
export CONTECO_DC_GLOBAL_BOOTSTACKS="conteco_dc_dotnetcore_runtime22"
export CONTECO_DC_GLOBAL_RUNSTACKS=""
export CONTECO_DC_GLOBAL_VOLUME_LIST="${DOLLAR}{CONTECO_STACKPREFIX}_dotnetcore_runtime22_volume"
export CONTECO_DC_GLOBAL='$CONTECO_DC_GLOBAL_SERVICE_NAME,$CONTECO_DC_GLOBAL_IMAGE_NAME,$CONTECO_DC_DOTNETCORE_RUNTIME22_PORTS,$CONTECO_DC_DOTNETCORE_RUNTIME22_NETWORKS,$CONTECO_DC_DOTNETCORE_RUNTIME22_VOLUMES,$CONTECO_DC_GLOBAL_VOLUMES,$CONTECO_DC_GLOBAL_NETWORKS'
# END CONFIGURATION

# START GLOBAL SETTINGS
export CONTECO_DC_GLOBAL_SERVICE_NAME="${CONTECO_TYPE}"
export CONTECO_DC_GLOBAL_IMAGE_NAME="${CONTECO_TYPE}.${CONTECO_NAME}:${CONTECO_TAG}"
export CONTECO_DC_DOTNETCORE_RUNTIME22_PORTS=
export CONTECO_DC_DOTNETCORE_RUNTIME22_NETWORKS="${DOLLAR}{CONTECO_NETWORKLOCAL}_overlay"
export CONTECO_DC_DOTNETCORE_RUNTIME22_VOLUMES="'${DOLLAR}{CONTECO_STACKPREFIX}_dotnetcore_runtime22_volume:/app'"
export CONTECO_DC_GLOBAL_VOLUMES="${DOLLAR}{CONTECO_STACKPREFIX}_dotnetcore_runtime22_volume: { external: true }"
export CONTECO_DC_GLOBAL_NETWORKS="${DOLLAR}{CONTECO_NETWORKLOCAL}_overlay: { external: true }"
# END GLOBAL SETTINGS

# START SETTINGS artifactory.base
export CONTECO_DC_DOTNETCORE_RUNTIME22_ENVIRONMENT=
export CONTECO_DC_DOTNETCORE_RUNTIME22_COMMAND="--keepalive:0"
export CONTECO_DC_DOTNETCORE_RUNTIME22_DEPLOY_LABELS=
export CONTECO_DC_DOTNETCORE_RUNTIME22_DEPLOY_MODE="replicated"
export CONTECO_DC_DOTNETCORE_RUNTIME22_DEPLOY_RESOURCES_LIMITS_CPUS=
export CONTECO_DC_DOTNETCORE_RUNTIME22_DEPLOY_RESOURCES_RESERVATIONS_CPUS="0.0"
export CONTECO_DC_DOTNETCORE_RUNTIME22_DEPLOY_RESOURCES_LIMITS_MEMORY="24m"
export CONTECO_DC_DOTNETCORE_RUNTIME22_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="5m"
# END SETTINGS artifactory.base

# START TOPLEVEL SETTINGS
# END TOPLEVEL SETTINGS
