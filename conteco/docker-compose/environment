#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export CONTECO_DC_GLOBAL_STACKS="dotnetcore_RUNTIME31.base"
export CONTECO_DC_GLOBAL_STACKNAMES="conteco_dc_dotnetcore_RUNTIME31"
export CONTECO_DC_GLOBAL_BOOTSTACKS="conteco_dc_dotnetcore_RUNTIME31"
export CONTECO_DC_GLOBAL_RUNSTACKS=""
export CONTECO_DC_GLOBAL_VOLUME_LIST="${DOLLAR}{CONTECO_STACKPREFIX}_dotnetcore_RUNTIME31_volume"
export CONTECO_DC_GLOBAL='$CONTECO_DC_GLOBAL_SERVICE_NAME,$CONTECO_DC_GLOBAL_IMAGE_NAME,$CONTECO_DC_DOTNETCORE_RUNTIME31_PORTS,$CONTECO_DC_DOTNETCORE_RUNTIME31_NETWORKS,$CONTECO_DC_DOTNETCORE_RUNTIME31_VOLUMES,$CONTECO_DC_GLOBAL_VOLUMES,$CONTECO_DC_GLOBAL_NETWORKS'
# END CONFIGURATION

# START GLOBAL SETTINGS
export CONTECO_DC_GLOBAL_SERVICE_NAME="${CONTECO_TYPE}"
export CONTECO_DC_GLOBAL_IMAGE_NAME="${CONTECO_TYPE}.${CONTECO_NAME}:${CONTECO_TAG}"
export CONTECO_DC_DOTNETCORE_RUNTIME31_PORTS=
export CONTECO_DC_DOTNETCORE_RUNTIME31_NETWORKS="${DOLLAR}{CONTECO_NETWORKLOCAL}_overlay"
export CONTECO_DC_DOTNETCORE_RUNTIME31_VOLUMES="'${DOLLAR}{CONTECO_STACKPREFIX}_dotnetcore_RUNTIME31_volume:/app'"
export CONTECO_DC_GLOBAL_VOLUMES="${DOLLAR}{CONTECO_STACKPREFIX}_dotnetcore_RUNTIME31_volume: { external: true }"
export CONTECO_DC_GLOBAL_NETWORKS="${DOLLAR}{CONTECO_NETWORKLOCAL}_overlay: { external: true }"
# END GLOBAL SETTINGS

# START SETTINGS artifactory.base
export CONTECO_DC_DOTNETCORE_RUNTIME31_ENVIRONMENT=
export CONTECO_DC_DOTNETCORE_RUNTIME31_COMMAND="--keepalive:0"
export CONTECO_DC_DOTNETCORE_RUNTIME31_LABELS=
export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_LABELS=
export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_REPLICAS=1
export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_PLACEMENT_CONSTRAINTS=
export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_MODE="replicated"
if [[ "${CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_MODE}" == "replicated" ]] ; then export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_MODE_REPLICATED_REPLICAS="replicas: ${CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_REPLICAS}"; else export export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_MODE_REPLICATED_REPLICAS= ; fi
export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_RESOURCES_LIMITS_CPUS=
export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_RESOURCES_RESERVATIONS_CPUS="0.0"
export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_RESOURCES_LIMITS_MEMORY="24m"
export CONTECO_DC_DOTNETCORE_RUNTIME31_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="5m"
# END SETTINGS artifactory.base

# START TOPLEVEL SETTINGS
# END TOPLEVEL SETTINGS
